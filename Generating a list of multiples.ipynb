    "def multiples(num, length):\n",
    "    result = []\n",
    "    for i in range(1, length+1):\n",
    "        result.append(num * i)\n",
    "    return result\n",
    "\n",
    "print (multiples(3,7))\n",
    "print (multiples(10,4))"
   ]
